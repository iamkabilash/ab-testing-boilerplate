{
  "name": "ab-testing-boilerplate",
  "version": "1.0.0",
  "description": "Echo A/B testing boilerplate with SCSS, Babel, LiveReload support.",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "serve": "webpack serve --mode=development",
    "generate-files": "node node_scripts/generate-files.js",
    "create-client": "node node_scripts/create-client.js",
    "create-site": "node node_scripts/create-site.js",
    "create-project": "node node_scripts/create-project.js",
    "select-project": "node node_scripts/select-project.js",
    "cli": "node node_scripts/cli.js",
    "dev": "npm-run-all -s serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saabbir/echo-ab-testing-boilerplate.git"
  },
  "keywords": [
    "A/B Testing",
    "Boilerplate",
    "EchoLogyx"
  ],
  "author": "Saabbir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Saabbir/echo-ab-testing-boilerplate/issues"
  },
  "homepage": "https://github.com/Saabbir/echo-ab-testing-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "css-loader": "^6.2.0",
    "livereload": "^0.9.3",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-gap-properties": "^2.0.0",
    "postcss-loader": "^6.1.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-glob-entry": "^2.1.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "not ie < 9",
    "iOS >= 9"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "fs-extra": "^10.1.0",
    "http-proxy": "^1.18.1",
    "is-invalid-path": "^1.0.2",
    "is-valid-path": "^0.1.1",
    "prompt": "^1.3.0"
